  },
  "homepage": "https://github.com/Mafrans/StadiaPlusDB#readme",
  "dependencies": {
    "@types/vue": "^2.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "css-loader": "^4.2.1",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "file-loader": "^6.0.0",
    "mongodb": "^3.6.0",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
